{
  "name": "@mukea/electron-callback-rpc",
  "version": "0.1.3",
  "description": "rpc for electron",
  "main": "./dist/main/index.js",
  "types": "./dist/main/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "browser": "./dist/renderer/index.d.ts", 
        "node": "./dist/main/index.d.ts",
        "default": "./dist/main/index.d.ts"
      },
      "browser": {
        "import": "./dist/renderer/index.mjs",
        "require": "./dist/renderer/index.js",
        "default": "./dist/renderer/index.mjs"
      },
      "node": {
        "import": "./dist/main/index.mjs",
        "require": "./dist/main/index.js",
        "default": "./dist/main/index.js"
      },
      "default": "./dist/main/index.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
